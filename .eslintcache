atedRules":"58"},"/Users/seanlee/Desktop/first_project/copymind-client/src/index.js",[],["116","117"],"/Users/seanlee/Desktop/first_project/copymind-client/src/App.js",["118"],"import React from 'react';\r\nimport {Switch, Route} from 'react-router-dom'\r\nimport './App.css';\r\nimport {PostCopy,Signup, MyProfile, Login } from './components'\r\nimport {_Main, _ListCopy, _ViewCopy, _Nav} from './containers'\r\n\r\nclass App extends React.Component {\r\n  state={\r\n    menu:['Novel', 'Essay', 'Quotes', 'Poem'],\r\n    isMyProfileOpen:false,\r\n    isLoginOpen:false,\r\n    isSignupOpen:false\r\n  }\r\n\r\n  modalOpen (value) {\r\n    if (value === 'login') this.setState({isLoginOpen:true})\r\n    else if (value === 'signup') this.setState({isSignupOpen:true})\r\n    else if (value === 'myprofile') this.setState({isMyProfileOpen:true})\r\n  }\r\n\r\n  modalClose (value) {\r\n    if (value === 'login') this.setState({isLoginOpen:false})\r\n    else if (value === 'signup') this.setState({isSignupOpen:false})\r\n    else if (value === 'myprofile') this.setState({isMyProfileOpen:false})\r\n  }\r\n\r\n  render () {\r\n    let menuPath = this.state.menu.map(el => '/'.concat(el.toLowerCase()))\r\n    \r\n    return (\r\n      <div>\r\n        <div className='header'>\r\n          <_Nav menu={this.state.menu} modalOpen={this.modalOpen.bind(this)} />\r\n        </div>\r\n        <div className='main'>\r\n          {\r\n          this.state.isMyProfileOpen?\r\n          (<MyProfile modalClose={this.modalClose.bind(this)} />)\r\n          :\r\n          (this.state.isLoginOpen?\r\n            (<Login modalClose={this.modalClose.bind(this)} modalOpen={this.modalOpen.bind(this)}/>)\r\n            :(this.state.isSignupOpen?\r\n            (<Signup modalClose={this.modalClose.bind(this)} modalOpen={this.modalOpen.bind(this)}/>)\r\n            :\r\n            <Switch>\r\n              <Route path={menuPath.concat('/myposting', '/bookmark')} component={_ListCopy} />\r\n              {/* <Route path='/login' component={_Login}/>\r\n              <Route path='/signup' component={Signup}/> */}\r\n              {/* <Route path='/myprofile' component={MyProfile}/> */}\r\n              <Route path='/view' component={_ViewCopy}/>\r\n              <Route path='/post' component={PostCopy} history={this.props.history}/>\r\n              <Route path='/' component={_Main} />\r\n            </Switch>))\r\n          }\r\n        </div>\r\n        <div className='footer'>\r\n          \r\n        </div>\r\n      </div>\r\n    );\r\n  } \r\n}\r\n\r\nexport default App;\r\n","/Users/seanlee/Desktop/first_project/copymind-client/src/store.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/serviceWorker.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/index.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/containers/index.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/reducers/index.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/ViewCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/PostCopy.js",["119","120","121","122"],"import React, {useState} from 'react';\nimport {useDispatch} from 'react-redux'\nimport './style/postCopy.css'\nimport {actionGetRandomCopy} from '../actions'\nimport axios from 'axios'\naxios.defaults.withCredentials=true;\n\nconst PostCopy = ({history}) => {\n    const [content, setContent] = useState('')\n    const [title, setTitle] = useState('')\n    const [writer, setWriter] = useState('')\n    const [category, setCategory] = useState('')\n\n    const dispatch = useDispatch()\n\n    const handleChange = (target) => {\n        switch(target.name) {\n            case 'content' :\n                setContent(target.value)\n            case 'title':\n                setTitle(target.value)\n            case 'writer':\n                setWriter(target.value)\n            case 'category':\n                setCategory(target.value)\n        }\n    }\n\n    const handleClickPost = () => {\n        let postData = {content, title, writer, category}\n        axios.post('http://13.209.5.235:8080/copy/postcopy',postData, {headers:{'Content-Type':'application/json'}})\n        .then(res => {\n            console.log(res.result)\n            if (res.message === 'ok') {\n                dispatch(actionGetRandomCopy(res.result[0]))\n                history.push('/view')\n            }\n        })\n    }\n\n    return (\n        <div className='post-copy'>\n            <div className='post-content'>\n                <textarea name=\"content\" id=\"\" cols='50' rows=\"30\" onChange={(e) => {handleChange(e.target)}}></textarea>\n            </div>\n            <div className='post-control'>\n                <div className='post-option'>\n                    <input name='title' type=\"text\" placeholder='Book Title'  onChange={(e) => {handleChange(e.target)}}/>\n                    <input name='writer' type=\"text\" placeholder='Writer' onChange={(e) => {handleChange(e.target)}}/>\n                    <select name=\"category\" id=\"\" onChange={(e) => {handleChange(e.target)}}>\n                        <option value=\"\">--none--</option>\n                        <option value=\"novel\">Novel</option>\n                        <option value=\"poem\">Poem</option>\n                        <option value=\"quotes\">Quotes</option>\n                        <option value=\"essay\">Essay</option>\n                    </select>\n                </div>\n                <div className='post-btn'>\n                    <input type=\"button\" value='Post' onClick={handleClickPost}/>\n                </div>\n            </div>\n        </div>\n    );\n};\n\n\n\nexport default PostCopy;","/Users/seanlee/Desktop/first_project/copymind-client/src/components/Signup.js",["123","124","125","126","127","128","129"],"import React, {useState} from 'react';\nimport {useDetectOutsideClick} from '../useDetectOutsideClick'\nimport './style/signup.css'\nimport {URL} from '../actions'\nimport axios from 'axios'\naxios.defaults.withCredentials=true;\n\nconst Signup = (props) => {\n    const [email, setEmail] = useState('')\n    const [userName, setUserName] = useState('')\n    const [password, setPassword] = useState('')\n    const [checkPW, setCheckPW] = useState('')\n    const [errorMessage, setErrorMessage] = useState('')\n\n    let userInfo = {email, userName, password, checkPW}\n\n\n    const handleChange = (target) => {\n        switch(target.name) {\n            case 'email' :\n                setEmail(target.value)\n            case 'userName':\n                setUserName(target.value)\n            case 'password':\n                setPassword(target.value)\n            case 'checkPW':\n                setCheckPW(target.value)\n        }\n    }\n\n\n    const signupHandle = () => {\n\n        if(!email || !password || !userName || !checkPW){\n            setErrorMessage(\"모든 항목은 필수입니다.\")\n        }else if(password !== checkPW){\n            setErrorMessage(\"비밀번호가 맞지않습니다.\")\n        }else if(!/^[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*\\.[a-zA-Z]{2,3}$/i.test(email)){\n            setErrorMessage(\"이메일 형식이 아닙니다.\")\n        }else if(!/^.*(?=^.{8,15}$)(?=.*\\d)(?=.*[A-Z])(?=.*[a-z])(?=.*[!@#$%^&+=]).*$/.test(password)){\n            setErrorMessage(\"비밀번호 형식이 아닙니다.\")\n        } else{\n            axios.post(`${URL}/sign/signup`,{email, userName, password},{headers:{'Content-Type':'application/json'}})\n            .then(res => {\n                console.log(res)\n            if(res.data.message === 'signup success!'){\n                props.modalClose('signup')\n                props.modalOpen('login')\n            }\n        })\n        .catch(err => console.log(err))\n        }\n    }\n\n    const signupModalRef = React.useRef(null)\n    useDetectOutsideClick(signupModalRef, () => props.modalClose('signup'))\n\n    return (\n        <div ref={signupModalRef}>\n            <input id=\"signup_email\" type=\"email\" placeholder=\"Email\" name=\"email\" onChange={(e) => handleChange(e.target)}/>\n\n            <input id=\"signup_username\" type=\"text\" placeholder=\"User Name\" name=\"userName\" onChange={(e) => handleChange(e.target)}/> \n            \n            <input id=\"signup_pw\" type=\"password\" placeholder=\"Password\" name=\"password\" onChange={(e) => handleChange(e.target)}/> \n\n            <input id=\"signup_confirmPW\" type=\"password\" placeholder=\"Confirm Password\" name=\"checkPW\" onChange={(e) => handleChange(e.target)}/>\n\n            <button id=\"signup_button\" onClick={signupHandle}>Signup</button>\n            {errorMessage?<div className=\"alert-box\">{errorMessage}</div>:null}\n        </div>\n    )\n};\n\nexport default Signup;","/Users/seanlee/Desktop/first_project/copymind-client/src/components/BtnViewCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/BtnPostCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/MyProfile.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/containers/_ListCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/containers/_ViewCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/containers/_Nav.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/reducers/viewCopyReducer.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/reducers/loginReducer.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/reducers/listCopyReducer.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/reducers/myProfileReducer.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/ListCopy.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/containers/_Main.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/Login.js",["130"],"import React from 'react'\nimport './style/login.css'\nimport {useState} from 'react'\nimport {useDispatch} from 'react-redux'\nimport {URL, actionLogin} from '../actions'\nimport axios from 'axios'\nimport {useDetectOutsideClick} from '../useDetectOutsideClick'\naxios.defaults.withCredentials=true\n\n\n\nexport default function Login(props)  {\n        const [inputs, setInputs] = useState({\n            email:'',\n            password:''\n        })\n        const {email,  password} = inputs\n        const dispatch = useDispatch()\n\n        const onChange = (e) => {\n            const {email, password} = e.target\n            e.target.value.replace(/</g, '&lt')\n            e.target.value.replace(/>/g, '&gt')\n            setInputs({\n                ...inputs,\n                [email]:password\n            })\n        }\n\n        const loginHandler = (postData) => {\n            axios.post(`${URL}/sign/signin`,postData,{headers:{'Content-Type': 'application/json'}})\n            .then(res =>{ \n                if(res.data.message === 'ok'){\n                    dispatch(actionLogin(true))\n                }\n            })\n        }\n\n        const loginModalRef = React.useRef(null)\n        useDetectOutsideClick(loginModalRef, () => {props.modalClose('login')})\n\n        return(\n            <div className=\"view\" >\n                <div id=\"loginInfo\" >\n                    <div id=\"loginInfo_border\" ref={loginModalRef}>\n                        <div id=\"input\">\n                            <div>\n                                <input type= \"email\" id='email' name='email' placeholder=\"Email\" value={email} onChange={onChange}/>\n                            </div>\n                            <div>\n                                <input type= \"password\" id='pw' name='password' placeholder=\"Password\" value={password} onChange={onChange}/>\n                            </div>\n                        </div>\n                        <div className=\"buttons\">\n                            <div id=\"LoginButton1\"\n                            onClick={()=>{loginHandler(inputs)}}>\n                                <span>Login</span>\n                                <span>Login</span>\n                            </div>\n                            <div id=\"LoginButton2\"\n                            onClick={()=>{loginHandler(inputs)}}>\n                                LOG IN\n                            </div>\n                            <div id=\"__SignUp__\">\n                                <input type=\"submit\" id=\"SignUp3\" value=\"SignUp\"\n                                onClick={() => {\n                                    props.modalClose('login')\n                                    props.modalOpen('signup')\n                                }}\n                                />\n                                <input  type=\"submit\"  className=\"SignUp1\"  value=\"SignUp\" id=\"SignUp\"\n                                    />\n                            </div>\n                        </div>\n                    </div>\n                    <div id=\"oauth-button\">\n                        <a href=\"#\" className=\"btn btn-google-plus\"><i className=\"fa fa-google-plus\"></i>  Sign in with Google+</a>\n                        <input id=\"OAuth-Github\" type=\"button\" value=\"LOGIN WITH GITHUB\" />\n                    </div>\n                </div>\n            </div>\n        )\n}","/Users/seanlee/Desktop/first_project/copymind-client/src/components/Main.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/components/Nav.js",[],"/Users/seanlee/Desktop/first_project/copymind-client/src/actions/index.js",[],["131","132"],"/Users/seanlee/Desktop/first_project/copymind-client/src/useDetectOutsideClick.js",[],{"ruleId":"133","replacedBy":"134"},{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","severity":1,"message":"138","line":33,"column":11,"nodeType":"139","endLine":33,"endColumn":80},{"ruleId":"140","severity":1,"me