n        }else if(!/^[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*@[0-9a-zA-Z]([-_\\.]?[0-9a-zA-Z])*\\.[a-zA-Z]{2,3}$/i.test(Email)){\n            this.setState({errorMessage : \"이메일 형식이 아닙니다.\"})\n        }else if(!/^.*(?=^.{8,15}$)(?=.*\\d)(?=.*[A-Z])(?=.*[a-z])(?=.*[!@#$%^&+=]).*$/.test(Password)){\n            this.setState({errorMessage : \"비밀번호 형식이 아닙니다.\"})\n        } else{\n            axios.post('http://localhost:8080/sign/signup',{Email:Email, Name:Name, Password:Password},{headers:{'Content-Type':'application/json'}})\n            .then(res => {\n                console.log(res)\n            if(res.data.message === 'signup success!'){\n                this.setState({isSignup : true})\n            }\n        })\n        .catch(err => console.log(err))\n        }\n    }\n\n\n\n    render(){\n\n        return (\n            <div>\n                <input id=\"signup_email\" type=\"email\" placeholder=\"Email\" name=\"Email\" value={this.state.userInfo.Email} onChange={this.changeHandler.bind(this)}/>\n    \n                <input id=\"signup_username\" type=\"text\" placeholder=\"User Name\" name=\"Name\" value={this.state.userInfo.Name} onChange={this.changeHandler.bind(this)}/> \n                \n                <input id=\"signup_pw\" type=\"password\" placeholder=\"Password\" name=\"Password\" value={this.state.userInfo.Password} onChange={this.changeHandler.bind(this)}/> \n    \n                <input id=\"signup_confirmPW\" type=\"password\" placeholder=\"Confirm Password\" name=\"checkPW\" value={this.state.userInfo.checkPW} onChange={this.changeHandler.bind(this)}/>\n    \n                <button \n                id=\"signup_button\" \n                onClick={\n                        ()=>{this.signupHandle(this.state.userInfo)}\n                        }>\n                        Signup</button>\n                {this.state.errorMessage?<div className=\"alert-box\">\n                  {this.state.errorMessage}\n                </div>:null}\n            </div>\n        )\n    }\n    \n}","/home/bsy/Desktop/codestates/copyMind-client/src/components/index.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/ListCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/PostCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/ViewCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/Main.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/BtnViewCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/Nav.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/BtnPostCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/components/MyProfile.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/store.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/index.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/actions/index.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/containers/_Login.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/containers/index.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/test.js",[],["133","134"],"/home/bsy/Desktop/codestates/copyMind-client/src/containers/_Main.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/containers/_ListCopy.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/loginReducer.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/postCopyReducer.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/viewCopyReducer.js",[],"/home/bsy/Desktop/codestates/copyMind-client/src/reducers/listCopyReducer.js",[],{"ruleId":"135","replacedBy":"136"},{"ruleId":"137","replacedBy":"138"},{"ruleId":"135","replacedBy":"139"},{"ruleId":"137","replacedBy":"140"},{"ruleId":"141","severity":1,"message":"142","line":35,"column":25,"nodeType":"143","endLine":35,"endColumn":65},{"ruleId":"141","severity":1,"message":"142","line":37,"column":25,"nodeType":"143","endLine":37,"endColumn":